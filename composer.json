{
    "name": "simplestart/start-library",
    "description": "Simplestart Library For Thinkphp V6.0 ",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Colber.Dun",
            "email": "colber@simplestart.cn",
            "homepage": "http://www.simplestart.cn"
        }
    ],
    "require": {
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "topthink/framework": "^6.0"
    },
    "replace": {
        "topthink/think-multi-app": "^1.0"
    },
    "autoload": {
        "files": [
          "src/Function.php"
        ],
        "psr-4": {
          "start\\": "src"
        }
    },
    "extra": {
        "think": {
          "services": [
            "start\\Library"
          ]
        }
    }
}
