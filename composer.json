{
    "name": "simplestart/start-library",
    "description": "Simplestart Library For Thinkphp V6.0 ",
    "version": "v1.0.4",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Colber.Dun",
            "email": "colber@simplestart.cn",
            "homepage": "http://www.simplestart.cn"
        }
    ],
    "require": {
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "topthink/framework": "^6.0",
        "qiniu/php-sdk": "^7.2",
        "aliyuncs/oss-sdk-php": "^2.3",
        "qcloud/cos-sdk-v5": "^1.2"
    },
    "replace": {
        "topthink/think-multi-app": "^1.0"
    },
    "autoload": {
        "files": [
          "src/Function.php"
        ],
        "psr-4": {
          "start\\": "src"
        }
    },
    "extra": {
        "think": {
          "services": [
            "start\\Library"
          ]
        }
    }
}
